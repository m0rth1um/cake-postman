{
    "name": "scherersoftware/cake-postman",
    "description": "CakePHP postman plugin",
    "license": "MIT",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakephp/plugin-installer": "*",
        "codekanzlei/cake-frontend-bridge": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakePostman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePostman\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
